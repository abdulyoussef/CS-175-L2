 
public class RoachSim {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		RoachPopulation RoachPop = new RoachPopulation (10); 
		
		System.out.println("The initial Roach population is " + RoachPop.getRoaches() );
		
		RoachPop.breed(); 
		
		System.out.println("The Roach population after cycle 1 of breeding is: " + RoachPop.getRoaches());
		
		RoachPop.spray(50);
		
		System.out.println("The Roach population after cycle 1 50% spraying is: " + RoachPop.getRoaches());
		
		RoachPop.breed(); 
		
		System.out.println("The Roach population after cycle 2 of breeding is: " + RoachPop.getRoaches());
		
		RoachPop.spray(60);
		
		System.out.println("The Roach population after cycle 2 60% spraying is: " + RoachPop.getRoaches());
		
		RoachPop.breed(); 
		
		System.out.println("The Roach population after cycle 3 of breeding is: " + RoachPop.getRoaches());
		
		RoachPop.spray(70);
		
		System.out.println("The Roach population after cycle 3 70% spraying is: " + RoachPop.getRoaches());
		
		RoachPop.breed();
		
		System.out.println("The Roach population after cycle 4 of breeding is: " + RoachPop.getRoaches());
		
		RoachPop.spray(80);
		
		System.out.println("The Roach population after cycle 4 80% spraying is: " + RoachPop.getRoaches());

	}

}
